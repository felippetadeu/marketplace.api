{
    "application": {
        "name": "Worflow",
        "default_logger": "console"
    },
    "logging": {
        "level":"DEBUG",
        "handlers": [
            {
                "class": "StreamHandler",
                "formatter": "%(asctime)s - %(levelname)s - %(message)s"
            }
        ],
        "teams_channel": {
            "webhook_url": "https://outlook.office.com/webhook/..."
        }
    }
}